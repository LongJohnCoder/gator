tag a is vec<2>;
tag b is vec<2>;
tag c is vec<2>;
tag d is vec<3>;

`u app<`t : vec<2>, `u : vec<2>>(`t v, `t->`u m) {
    return v in `u;
}

canon d f<`t : vec<2>>(`t x) {
    return [1., 2., 3.];
}

canon d g(c x) { // this is legal since it is more specific than f
    return [2., 2., 2.];
}

void main() {
    a avec = [1., 0.];
    b bvec = [0., 1.];
    c cvec = [1., 1.];

    a->b atb = [[0., 1.], [1., 0.]];

    b s1 = app<a, b>(avec, atb);
    a s2 = app<a, a>(avec, [[1., 0.], [0., 1.]]);
    d s3 = cvec in d; // g(cvec)

    print s3;
}