space a is vec<2>;
space b is vec<3>;
tag c is b;
tag d is b;

void main() {
    a x = [1., 2.];
    b y = [2., 1., 2.];
    c z = [3., 3., 3.];
    d z2 = [2., 2., 2.];

    print x;
    print y;
    print z;
    print z2;

    // Vector safety
    a s11 = x + [2., 1.];
    b s12 = z + z2;
    b s13 = z * 2.;

    print s11;
    print s12;
    print s13;

    vec<2>->d v2_d = [[1., 1.], [1., 1.], [1., 1.]];
    a->b a_b = v2_d;
    vec<3>->c v3_c = [[1., 1., 1.], [1., 1., 1.], [1., 1., 1.]];

    // Matrix safety
    c->b s21 = v3_c * 2.;
    vec<2>->d s22 = v2_d + [[1., 1.], [1., 1.], [1., 1.]];
    a->b s23 = a_b + v2_d;

    print s21;
    print s22;
    print s23;
    print (a_b .* (v2_d * 2.)); // There is no writeable variable type to assign this to
}