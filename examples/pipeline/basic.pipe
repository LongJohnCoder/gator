int factor;

void main() {
    with multiplier {
        bind 3 to factor {
            int[3] result;
            bind [1, 2, 3] to multiplier.var1 {
                run() {
                    result = multiplier.var2;
                }
            }
            print result;
        }
    }   
}

program multiplier {
    in int var1;
    out int var2;
    void main() {
        var2 = var1 * factor;
    }
}